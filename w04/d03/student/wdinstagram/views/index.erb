<div class="feed box">
  <div class="entry">
    <% @entries.each do |entry| %>
    <span class="author-text"><%= entry["author"] %></span>
    <span class="date-text"><%= entry["date_taken"] %></span>
    <!-- <hr class="before"/> -->

    <img alt="Entry Image" src="<%= entry["photo_url"] %>" />
    <!-- <img alt="Entry Image" src="/images/...png" /> -->
    <!-- <hr class="after"/> -->
    <span class="likes-text"><i class="fa fa-heart"></i> ... likes</span>
  </div>
  <% end %>
</div>
